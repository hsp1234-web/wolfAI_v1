    "import os\n",
    "import sys\n",
    "import shutil\n",
    "import time\n",
    "import requests # 用於未來的健康檢查 (如果重新加入)\n",
    "import select\n",
    "import subprocess\n",
    "from IPython.display import display, HTML, clear_output\n",
    "\n",
    "# 清理先前執行可能產生的輸出\n",
    "clear_output(wait=True)\n",
    "\n",
    "print(\"🚀 蒼狼 AI V2.2 Colab 啟動程序開始...\")\n",
    "print(\"=========================================\")\n",
    "\n",
    "# --- 1. 設定環境變數 --- \n",
    "print(\"\\n⚙️ 1. 正在根據您的選擇設定環境變數...\")\n",
    "if operation_mode_choice == \"持久模式 (需 Google Drive 授權)\":\n",
    "    OPERATION_MODE = \"persistent\"\n",
    "else:\n",
    "    OPERATION_MODE = \"transient\"\n",
    "os.environ['OPERATION_MODE'] = OPERATION_MODE\n",
    "\n",
    "if launch_mode == \"除錯模式 (日誌即時輸出)\":\n",
    "    MODE = \"debug\"\n",
    "else:\n",
    "    MODE = \"normal\"\n",
    "os.environ['APP_LAUNCH_MODE'] = MODE # start.sh 會使用 APP_LAUNCH_MODE, 但後端可能直接讀 MODE\n",
    "os.environ['MODE'] = MODE # 確保後端也能讀取\n",
    "\n",
    "print(f\"   設定程式碼分支: {branch_name}\")\n",
    "print(f\"   設定操作模式 (OPERATION_MODE): {OPERATION_MODE} (來源: '{operation_mode_choice}')\")\n",
    "print(f\"   設定啟動模式 (MODE): {MODE} (來源: '{launch_mode}')\")\n",
    "\n",
    "# --- 2. 掛載 Google Drive (持久模式下) --- \n",
    "if OPERATION_MODE == \"persistent\":\n",
    "    print(\"\\n💾 2. 持久模式：正在掛載 Google Drive...\")\n",
    "    try:\n",
    "        from google.colab import drive\n",
    "        drive.mount('/content/drive', force_remount=True) # 強制重新掛載以避免陳舊狀態\n",
    "        print(\"   ✅ Google Drive 已成功掛載到 /content/drive\")\n",
    "    except Exception as e:\n",
    "        print(f\"   ❌ 錯誤：掛載 Google Drive 失敗: {e}\")\n",
    "        print(\"      請檢查彈出視窗是否已正確授權，或手動執行左側「掛載 Drive」按鈕。\")\n",
    "        print(\"      持久模式下，Drive 掛載失敗將導致後續步驟無法正確設定，建議解決後重試。\")\n",
    "        sys.exit(\"Google Drive 掛載失敗，程序終止。\") # 終止腳本\n",
    "else:\n",
    "    print(\"\\n💾 2. 暫存模式：跳過 Google Drive 掛載。\")\n",
    "\n",
    "# --- 3. Colab Secrets 檢查與資料庫路徑設定 --- \n",
    "print(\"\\n🔑 3. 正在檢查 Colab Secrets 並設定資料庫路徑...\")\n",
    "all_secrets_ok = True\n",
    "\n",
    "# 3.1 檢查 COLAB_GOOGLE_API_KEY (AI 服務金鑰)\n",
    "print(\"   檢查 COLAB_GOOGLE_API_KEY (AI 服務金鑰)...\")\n",
    "google_api_key_env = os.getenv('COLAB_GOOGLE_API_KEY')\n",
    "if not google_api_key_env:\n",
    "    print(\"     ⚠️ COLAB_GOOGLE_API_KEY 未設定。AI 分析功能可能無法使用。建議在 Colab Secrets 中設定此金鑰。\")\n",
    "    # 在持久模式下，如果沒有 AI 金鑰，也視為一個重要警告，但不一定中止\n",
    "    if OPERATION_MODE == \"persistent\":\n",
    "        print(\"     ❗ 持久模式下，強烈建議設定 COLAB_GOOGLE_API_KEY 以確保 AI 功能完整。\")\n",
    "        # all_secrets_ok = False # 取決於是否要強制中止，目前僅警告\n",
    "else:\n",
    "    print(\"     ✅ COLAB_GOOGLE_API_KEY 已設定。\")\n",
    "\n",
    "# 3.2 持久模式下的特定 Secrets 檢查\n",
    "project_name_for_db = \"wolf_ai_v2_2\" # 專案目錄名稱，用於路徑\n",
    "project_parent_dir_for_db = \"/content\" # 專案複製的父目錄\n",
    "default_project_path_for_db = os.path.join(project_parent_dir_for_db, project_name_for_db)\n",
    "\n",
    "if OPERATION_MODE == \"persistent\":\n",
    "    print(\"   持久模式：檢查 Google Drive 相關 Colab Secrets...\")\n",
    "    secrets_to_check_persistent = [\n",
    "        'GOOGLE_SERVICE_ACCOUNT_JSON_CONTENT',\n",
    "        'WOLF_IN_FOLDER_ID',\n",
    "        'WOLF_PROCESSED_FOLDER_ID'\n",
    "    ]\n",
    "    missing_secrets_persistent = []\n",
    "    for secret_name in secrets_to_check_persistent:\n",
    "        secret_value = os.getenv(secret_name)\n",
    "        if not secret_value:\n",
    "            missing_secrets_persistent.append(secret_name)\n",
    "            print(f\"     ❌ {secret_name} 未設定或為空值！\")\n",
    "        else:\n",
    "            print(f\"     ✅ {secret_name} 已設定。\")\n",
    "\n",
    "    if missing_secrets_persistent:\n",
    "        print(\"\\n     ⚠️ **警告：持久模式下，一個或多個必要的 Google Drive Colab Secret 未被正確設定！**\")\n",
    "        all_secrets_ok = False # 標記設定不完整\n",
    "    else:\n",
    "        print(\"\\n     ✅ 持久模式下所有必要的 Google Drive Colab Secrets 看起來都已設定。\")\n",
    "\n",
    "    # 設定持久模式下的資料庫路徑 (Google Drive)\n",
    "    # 假設使用者的 Google Drive 根目錄下名為 MyDrive (此為 Colab 掛載後的常見路徑)\n",
    "    drive_mounted_base_path = \"/content/drive/MyDrive/wolf_AI_data\" # 資料庫存放的基礎路徑\n",
    "    reports_db_file = os.getenv('REPORTS_DB_FILENAME', 'reports.sqlite')\n",
    "    prompts_db_file = os.getenv('PROMPTS_DB_FILENAME', 'prompts.sqlite')\n",
    "    os.environ['REPORTS_DB_PATH'] = os.path.join(drive_mounted_base_path, reports_db_file)\n",
    "    os.environ['PROMPTS_DB_PATH'] = os.path.join(drive_mounted_base_path, prompts_db_file)\n",
    "    print(f\"     環境變數 REPORTS_DB_PATH 已設定為 (持久模式): {os.getenv('REPORTS_DB_PATH')}\")\n",
    "    print(f\"     環境變數 PROMPTS_DB_PATH 已設定為 (持久模式): {os.getenv('PROMPTS_DB_PATH')}\")\n",
    "\n",
    "elif OPERATION_MODE == \"transient\":\n",
    "    print(\"   暫存模式：跳過 Google Drive 相關 Colab Secrets 的深入檢查。\")\n",
    "    # 設定暫存模式下的資料庫路徑 (Colab 本地)\n",
    "    base_data_path_for_db = os.path.join(default_project_path_for_db, 'data')\n",
    "    reports_db_file_transient = os.getenv('REPORTS_DB_FILENAME', 'session_reports.sqlite')\n",
    "    prompts_db_file_transient = os.getenv('PROMPTS_DB_FILENAME', 'session_prompts.sqlite')\n",
    "    os.environ['REPORTS_DB_PATH'] = os.path.join(base_data_path_for_db, reports_db_file_transient)\n",
    "    os.environ['PROMPTS_DB_PATH'] = os.path.join(base_data_path_for_db, prompts_db_file_transient)\n",
    "    print(f\"     環境變數 REPORTS_DB_PATH 已設定為 (暫存模式): {os.getenv('REPORTS_DB_PATH')}\")\n",
    "    print(f\"     環境變數 PROMPTS_DB_PATH 已設定為 (暫存模式): {os.getenv('PROMPTS_DB_PATH')}\")\n",
    "\n",
    "if not all_secrets_ok:\n",
    "    print(\"\\n   ❌ **錯誤：Colab Secrets 設定不完整或不正確（根據所選操作模式），無法繼續。**\")\n",
    "    print(\"      請返回 Colab 的「🔑 密鑰」管理器進行設定，或調整操作模式，然後重新執行此儲存格。\")\n",
    "    sys.exit(\"Colab Secrets 設定不完整，程序終止。\")\n",
    "else:\n",
    "    print(\"\\n   ✅ Colab Secrets 設定檢查完畢 (根據操作模式)。\")\n",
    "\n",
    "# --- 4. 部署與啟動應用程式 --- \n",
    "print(\"\\n🚚 4. 正在部署並啟動應用程式...\")\n",
    "GIT_REPO_URL = \"https://github.com/hsp1234-web/wolfAI_v1.git\" # 應更新為 V2.2 的倉庫 URL\n",
    "PROJECT_PATH = default_project_path_for_db # 使用先前定義的專案路徑 /content/wolf_ai_v2_2\n",
    "\n",
    "print(f\"   專案將部署到：{PROJECT_PATH}\")\n",
    "if os.path.exists(PROJECT_PATH):\n",
    "    print(f\"   偵測到已存在的專案資料夾 '{PROJECT_PATH}'，將先移除以進行全新複製...\")\n",
    "    shutil.rmtree(PROJECT_PATH)\n",
    "    print(f\"   舊專案資料夾 '{PROJECT_PATH}' 已成功移除。\")\n",
    "\n",
    "print(f\"   開始從 {GIT_REPO_URL} 的 '{branch_name}' 分支複製專案...\")\n",
    "git_clone_command = [\"git\", \"clone\", \"--depth\", \"1\", \"-b\", branch_name, GIT_REPO_URL, PROJECT_PATH]\n",
    "clone_result = subprocess.run(git_clone_command, capture_output=True, text=True)\n",
    "\n",
    "if clone_result.returncode != 0:\n",
    "    print(f\"   ❌ 錯誤：專案複製失敗。返回碼: {clone_result.returncode}\")\n",
    "    print(f\"      Git Clone 標準輸出: {clone_result.stdout}\")\n",
    "    print(f\"      Git Clone 標準錯誤: {clone_result.stderr}\")\n",
    "    print(f\"      請檢查 GIT_REPO_URL ('{GIT_REPO_URL}') 和分支名稱 ('{branch_name}') 是否正確以及您的網路連線。\")\n",
    "    sys.exit(\"專案複製失敗，程序終止。\")\n",
    "else:\n",
    "    print(f\"   ✅ 專案已成功從 '{branch_name}' 分支複製到 '{PROJECT_PATH}' 目錄。\")\n",
    "\n",
    "if OPERATION_MODE == 'transient':\n",
    "    # 確保暫存模式下，資料庫檔案將要存放的 'data' 目錄存在\n",
    "    # REPORTS_DB_PATH 和 PROMPTS_DB_PATH 環境變數已指向此 'data' 目錄內的路徑\n",
    "    path_for_transient_db_data = os.path.join(PROJECT_PATH, 'data')\n",
    "    if not os.path.exists(path_for_transient_db_data):\n",
    "        os.makedirs(path_for_transient_db_data)\n",
    "        print(f\"   為暫存模式在專案內創建了資料目錄: {path_for_transient_db_data}\")\n",
    "\n",
    "start_script_path = os.path.join(PROJECT_PATH, 'scripts', 'start.sh')\n",
    "if not os.path.exists(start_script_path):\n",
    "    print(f\"   ❌ 錯誤：主控啟動腳本 {start_script_path} 未找到！無法繼續。\")\n",
    "    sys.exit(f\"啟動腳本 {start_script_path} 未找到，程序終止。\")\n",
    "\n",
    "print(f\"   賦予啟動腳本執行權限: chmod +x {start_script_path}\")\n",
    "os.chmod(start_script_path, 0o755) # rwxr-xr-x\n",
    "\n",
    "print(f\"   🎬 執行啟動腳本: {start_script_path} --mode={MODE}\")\n",
    "process = subprocess.Popen(\n",
    "    [start_script_path, f\"--mode={MODE}\"], \n",
    "    stdout=subprocess.PIPE, \n",
    "    stderr=subprocess.STDOUT, # 將 stderr 合併到 stdout\n",
    "    text=True, \n",
    "    bufsize=1,  # 行緩衝\n",
    "    cwd=PROJECT_PATH # 在專案目錄下執行\n",
    ")\n",
    "\n",
    "# --- 5. 日誌輸出處理 --- \n",
    "print(\"\\n📜 5. 正在處理應用程式日誌...\")\n",
    "if MODE == 'debug':\n",
    "    print(\"   除錯模式：即時輸出後端與前端日誌...\")\n",
    "    # 直接讀取 Popen 程序的輸出流\n",
    "    if process.stdout:\n",
    "        for line in iter(process.stdout.readline, ''):\n",
    "            print(f\"      [App Log]: {line.strip()}\")\n",
    "            if not line: # 當 Popen 結束時 readline 會返回空字串\n",
    "                break\n",
    "        process.stdout.close()\n",
    "    return_code = process.wait()\n",
    "    print(f\"   啟動腳本執行完畢，返回碼: {return_code}\")\n",
    "else: # normal mode\n",
    "    print(\"   一般模式：等待日誌檔案生成 (約 10-20 秒)...請耐心等候。\")\n",
    "    time.sleep(15) # 等待服務啟動並生成日誌檔案\n",
    "\n",
    "    backend_log_path = os.path.join(PROJECT_PATH, 'backend.log')\n",
    "    frontend_log_path = os.path.join(PROJECT_PATH, 'frontend.log')\n",
    "\n",
    "    # tail_logs 函數定義\n",
    "    def tail_logs(log_file_path, log_source_name):\n",
    "        print(f\"\\n   🔍 開始追蹤 {log_source_name} 日誌 ({log_file_path})...\")\n",
    "        try:\n",
    "            with open(log_file_path, 'r') as f:\n",
    "                # 移動到檔案末尾\n",
    "                f.seek(0, os.SEEK_END)\n",
    "                while True:\n",
    "                    # 檢查行程是否已結束，避免無限迴圈\n",
    "                    if process.poll() is not None:\n",
    "                        print(f\"   監控到 {log_source_name} 的父行程已結束。停止追蹤日誌。\")\n",
    "                        break\n",
    "                    line = f.readline()\n",
    "                    if not line:\n",
    "                        time.sleep(0.5) # 如果沒有新行，稍作等待\n",
    "                        continue\n",
    "                    print(f\"      [{log_source_name}]: {line.strip()}\")\n",
    "        except FileNotFoundError:\n",
    "            print(f\"   ⚠️ 警告：{log_source_name} 日誌檔案 {log_file_path} 未找到。可能服務啟動失敗或日誌路徑配置錯誤。\")\n",
    "        except Exception as e:\n",
    "            print(f\"   ❌ 追蹤 {log_source_name} 日誌時發生錯誤: {e}\")\n",
    "\n",
    "    log_files_exist = True\n",
    "    if not os.path.exists(backend_log_path):\n",
    "        print(f\"   ⚠️ 警告：後端日誌檔案 {backend_log_path} 未找到。\")\n",
    "        log_files_exist = False\n",
    "    if not os.path.exists(frontend_log_path):\n",
    "        print(f\"   ⚠️ 警告：前端日誌檔案 {frontend_log_path} 未找到。\")\n",
    "        log_files_exist = False\n",
    "\n",
    "    if log_files_exist:\n",
    "        print(\"   日誌檔案已找到。將開始追蹤日誌輸出。您可以隨時中斷此儲存格的執行以停止追蹤。\")\n",
    "        # 注意：在 Colab 中，tail_logs 函數的並行執行可能不直接支持，\n",
